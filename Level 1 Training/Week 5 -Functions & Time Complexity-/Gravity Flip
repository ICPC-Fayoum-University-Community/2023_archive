#include <bits/stdc++.h>

using namespace std;
#define ll long long
#define ld long double
#define speed ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);

void solve1(int n, int arr[])
{
    sort(arr, arr + n);
    for (int i = 0; i < n; i++)
        cout << arr[i] << ' ';
}

void solve()
{
    int n;
    int arr[101];
    cin >> n;
    for (int i = 0; i < n; i++)
        cin >> arr[i];
    solve1(n, arr);
}

int main()
{
    speed
#ifndef ONLINE_JUDGE
        freopen("in.txt", "r", stdin);
    freopen("out.txt", "w", stdout);
#endif

    //    int t;cin>>t;
    //    while(t--)
    solve();

    return 0;
}
